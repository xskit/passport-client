{
  "name": "xskit/passport-client",
  "description": "An authorized client based on the lararvel passport",
  "keywords": [
    "passport",
    "oauth",
    "authorization",
    "authorisation",
    "authentication",
    "oauth client",
    "sdk",
    "api",
    "laravel"
  ],
  "authors": [
    {
      "name": "兴顺",
      "email": "250915790@qq.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "~5.5",
    "illuminate/contracts": "~5.5",
    "illuminate/http": "~5.5"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "XsKit\\PassportClient\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "laravel": {
      "providers": [
        "XsKit\\PassportClient\\PassportClientServiceProvider"
      ],
      "facades": {
        "PassportClient": "XsKit\\PassportClient\\Facades\\PassportClient"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
