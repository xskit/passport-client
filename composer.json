{
  "name": "xspkg/passport-client",
  "description": "基于lararvel passport 的授权访问的客户端",
  "keywords": [
    "passport",
    "oauth",
    "authorization",
    "authorisation",
    "authentication",
    "oauth client",
    "sdk",
    "api",
    "laravel"
  ],
  "authors": [
    {
      "name": "兴顺",
      "email": "250915790@qq.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "illuminate/contracts": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
    "illuminate/routing": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
  },
  "require-dev": {
    "illuminate/http": "^5.8"
  },
  "autoload": {
    "psr-4": {
      "XsPkg\\PassportClient\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "laravel": {
      "providers": [
        "XsPkg\\PassportClient\\PassportClientServiceProvider"
      ],
      "facades": {
        "PassportClient": "XsPkg\\PassportClient\\Facade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
